<?xml version="1.0" encoding="utf-8" ?>
<!-- Sample configuration with no bank account fields
     fields in default order -->
<configuration>
	<configSections>
		<section name="DirectDebit" type="OrangeTentacle.DirectDebitAlbany.DirectDebitConfiguration, DirectDebitAlbany" />
	</configSections>

        <DirectDebit>
            <Record>
                <add field="Destination" />
                <add field="TransCode" />
                <add field="Originator" />
                <add field="Amount" />
                <add field="Reference" />
            </Record>
        </DirectDebit>
</configuration>
